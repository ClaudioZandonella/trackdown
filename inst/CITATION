citHeader("To cite trackdown in publications use:")

citEntry(entry = "Manual",
  title        = "trackdown: Collaborative Writing and Editing of R Markdown (or Sweave) Documents in Google Drive",
  author       = personList(as.person("Emily Kothe"),
                            as.person("Claudio Zandonella Callegher"),
                            as.person("Filippo Gambarota"),
                            as.person("Janosch Linkersdörfer"),
                            as.person(" Mathew Ling")),
  year         = "2021",
  note         = "R package version 1.1.1",
  url          = "https://github.com/claudiozandonella/trackdown",
  doi          = "10.5281/zenodo.5167320",

  textVersion  =
  paste("Emily Kothe, Claudio Zandonella Callegher, Filippo Gambarota, Janosch Linkersdörfer and Mathew Ling  (2021).",
        "trackdown: Collaborative Writing and Editing of R Markdown (or Sweave) Documents in Google Drive.",
        "https://doi.org/10.5281/zenodo.5167320")
)
